<span class="nb">print</span><span class="p">(</span><span class="s1">'Data Shape'</span><span class="p">)</span>
<span class="nb">print</span><span class="p">(</span><span class="n">boston</span><span class="p">.</span><span class="nf">data</span><span class="p">.</span><span class="nf">shape</span><span class="p">)</span>

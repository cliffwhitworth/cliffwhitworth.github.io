<span class="c1"># Create a dataframe of the data and add the Median Value target</span>
<span class="n">boston_dataframe</span> <span class="o">=</span> <span class="n">pd</span><span class="o">.</span><span class="no">DataFrame</span><span class="p">(</span><span class="n">boston</span><span class="p">.</span><span class="nf">data</span><span class="p">)</span>
<span class="n">boston_dataframe</span><span class="p">.</span><span class="nf">columns</span> <span class="o">=</span> <span class="n">boston</span><span class="p">.</span><span class="nf">feature_names</span>
<span class="n">boston_dataframe</span><span class="p">[</span><span class="s1">'MEDV'</span><span class="p">]</span> <span class="o">=</span> <span class="n">boston</span><span class="p">.</span><span class="nf">target</span>
<span class="nb">print</span><span class="p">(</span><span class="s1">'Dataframe Head'</span><span class="p">)</span>
